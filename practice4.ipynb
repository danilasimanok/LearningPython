    "def times(n):\n",
    "    def decor(a):\n",
    "        i=0\n",
    "        while i<n:\n",
    "            i+=1\n",
    "            a()\n",
    "        return None\n",
    "    return decor\n",
    "        \n",
    "@times(5)\n",
    "def A():\n",
    "    print(\"A\",end=\"\")"
